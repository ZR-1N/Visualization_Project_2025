{
    "version": 2,
    "builds": [
        {
            "src": "cv-explorer/backend/server.py",
            "use": "@vercel/python"
        },
        {
            "src": "cv-explorer/web/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/cv-explorer/backend/server.py"
        },
        {
            "src": "/",
            "dest": "/cv-explorer/web/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/cv-explorer/web/$1"
        }
    ]
}